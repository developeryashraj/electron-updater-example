{
  "name": "electron-update-example",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "electron-update-example",
    "win": {
      "target": "nsis",
      "publish": {
        "provider": "generic",
        "url": "http://localhost:5000/"
      }
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1"
  },
  "dependencies": {
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5"
  }
}
